{
    /* 
     * Enabled advanced config options
     * Additional options will be shown after next config load
     */
    EnableAdvancedConfig: false,

    /* 
     * Enable debug features, may lower performance
     * Default: false
     */
    debug: false,

    /* 
     * Backend mode used by quartz
     * Automatic will try to use the best available, and fallback as necessary
     * Default: Automatic
     * Allowed Values: null, Vulkan10, OpenGL46, OpenGL33, Automatic, 
     */
    mode: "Automatic",

    GL46: {
        /* 
         * Default: true
         */
        ALLOW_SPARSE_TEXTURE: true
    }
}